    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.map": true,
    "**/run": true,
    "**/logs": true,
    "**/out": true,
    "**/test": true,
    "**/.idea": true,
    "**/.autod.conf.js": true,
    "**/.eslintignore": true,
    "**/.eslintrc": true,
    // "**/.gitignore": true,
